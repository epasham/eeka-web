{
  "root": true,
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "html"
  ],
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-debugger": 2,
    "no-cond-assign": 2,
    "no-alert": 2,
    "no-unused-vars": 0,
    "generator-star-spacing": 0,
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": 2,
    "no-undef": [
      "off"
    ]
  }
}